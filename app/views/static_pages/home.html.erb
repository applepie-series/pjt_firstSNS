<% if logged_in? %>
  <% provide(:title, 'HOME') %>
  <div class="row home-container">
    <div class="col-md-2 home_user-container">
      <aside class="user_info-container">
        <section class="user_info">
          <%= render 'shared/user_info' %>
        </section>
        <section class="stats">
          <%= render 'shared/stats' %>
        </section>
      </aside>
    </div>
    <div class="col-md-10 feed-row">
      <%= render 'shared/feed' %>
    </div>
  </div>
<% else %>
  <%= redirect_to root_path%>
<% end %>