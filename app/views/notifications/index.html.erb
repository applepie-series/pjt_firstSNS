<% provide(:title, "通知一覧") %>
<% notifications = @notifications.where.not(visitor_id: current_user.id) %>
<div class="row notification-container">
  <% if notifications.present? %>
    <%= render notifications %>
    <%= will_paginate notifications %>
  <% else %>
    <p>| 通知はありません</p>
  <% end %> 
</div>
